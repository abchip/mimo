<?xml version="1.0" encoding="UTF-8"?>
<mimo-app:Application
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:biz="http://www.abchip.org/biz"
    xmlns:db="http://www.abchip.org/mimo/database"
    xmlns:mimo-app="http://www.abchip.org/mimo/application"
    xmlns:mimo-net="http://www.abchip.org/mimo/net"
    xsi:schemaLocation="http://www.abchip.org/biz ../../org.abchip.biz.core/model/biz.ecore http://www.abchip.org/mimo/database ../../org.abchip.mimo.database/model/database.ecore http://www.abchip.org/mimo/application ../../org.abchip.mimo.core/model/mimo.ecore#//application http://www.abchip.org/mimo/net ../../org.abchip.mimo.core/model/mimo.ecore#//net"
    name="abchip-net"
    text="ABChip-net Cloud Application">
  <commands
      className="org.abchip.mimo.core.e4.E4ApplicationCommandProviderImpl"/>
  <components
      name="Tester">
    <commands
        className="org.abchip.mimo.tester.base.BaseTesterCommandProviderImpl"/>
    <modules
        name="Base">
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.tester.base.BaseTestManagerImpl"
          interfaceName="org.abchip.mimo.tester.TestManager"/>
    </modules>
    <modules
        name="Test">
      <services
          xsi:type="mimo-app:ServiceTester"
          className="org.abchip.mimo.tester.test.TestTesterSuiteLauncherImpl"/>
    </modules>
  </components>
  <components
      name="Core">
    <hooks className="org.abchip.mimo.core.base.BaseEntityActivator"/>
    <commands
        className="org.abchip.mimo.core.base.BaseEntityCommandProviderImpl"/>
    <modules
        name="Base">
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.util.BaseStringsImpl"
          interfaceName="org.abchip.mimo.util.Strings"
          remoteExport="true"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.util.BaseListsImpl"
          interfaceName="org.abchip.mimo.util.Lists"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.util.BaseFilesImpl"
          interfaceName="org.abchip.mimo.util.Files"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.util.BaseStreamsImpl"
          interfaceName="org.abchip.mimo.util.Streams"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.util.BaseClassesImpl"
          interfaceName="org.abchip.mimo.util.Classes"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.util.BaseThreadsImpl"
          interfaceName="org.abchip.mimo.util.Threads"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.util.BaseThreadManagerImpl"
          interfaceName="org.abchip.mimo.util.ThreadManager"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.e4.E4LoggerImpl"
          interfaceName="org.abchip.mimo.context.Logger"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.BaseRegistryFactoryImpl"
          interfaceName="org.abchip.mimo.context.RegistryFactory"/>
    </modules>
    <modules
        name="Entity">
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.EMFFrameManagerImpl"
          interfaceName="org.abchip.mimo.entity.FrameManager"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.BaseLockManagerImpl"
          interfaceName="org.abchip.mimo.context.LockManager"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.BaseResourceManagerImpl"
          interfaceName="org.abchip.mimo.entity.ResourceManager"/>
      <services
          xsi:type="mimo-app:ServiceRegistry"
          className="org.abchip.mimo.core.base.BaseEntityProviderRegistryImpl"
          interfaceName="org.abchip.mimo.entity.EntityProviderRegistry">
        <entries
            className="org.abchip.mimo.EMFEntityProviderImpl"
            interfaceName="org.abchip.mimo.entity.EntityProvider"
            name="*EMF"
            vendor="ABChip"
            version="0.1.0"/>
        <entries
            className="org.abchip.mimo.core.base.nio.NIOEntityProviderImpl"
            interfaceName="org.abchip.mimo.entity.EntityProvider"
            name="*NIO"
            vendor="ABChip"
            version="0.1.0"/>
      </services>
    </modules>
    <modules
        name="Test">
      <services
          xsi:type="mimo-app:ServiceTester"
          className="org.abchip.mimo.core.test.TestCoreSuiteLauncherImpl"/>
    </modules>
  </components>
  <components
      name="Mining">
    <modules
        name="Base">
      <services
          xsi:type="mimo-app:ServiceRegistry"
          className="org.abchip.mimo.mining.base.BaseClassifierRegistryImpl"
          interfaceName="org.abchip.mimo.mining.classification.ClassifierRegistry"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.mining.base.BaseMiningManagerImpl"
          interfaceName="org.abchip.mimo.mining.MiningManager"/>
    </modules>
  </components>
  <components
      name="Database">
    <config>
      <entities
          xsi:type="db:DatabaseContainer"
          defaultCatalogContainer="MIMO_TEST"
          vendor="ABChip"
          version="0.1.0">
        <catalogContainers
            name="MIMO_TEST"
            supportsGuestAccess="true">
          <connectionConfig
              vendor="MySql"
              version="5.1"
              url="jdbc:mysql://localhost:3306/mimo_test"
              catalog="">
            <credentials
                user="root"
                password="mysql2017"/>
          </connectionConfig>
          <generationStrategy/>
        </catalogContainers>
      </entities>
    </config>
    <hooks className="org.abchip.mimo.database.base.BaseDatabaseActivator"
        status="STP"/>
    <modules
        name="Base">
      <services
          xsi:type="mimo-app:ServiceRegistry"
          className="org.abchip.mimo.database.base.BaseDefinitionParserRegistryImpl"
          interfaceName="org.abchip.mimo.database.definition.DefinitionParserRegistry"/>
      <services
          xsi:type="mimo-app:ServiceRegistry"
          className="org.abchip.mimo.database.base.BaseDefinitionWriterRegistryImpl"
          interfaceName="org.abchip.mimo.database.definition.DefinitionWriterRegistry"/>
      <services
          xsi:type="mimo-app:ServiceRegistry"
          className="org.abchip.mimo.database.base.BaseQueryParserRegistryImpl"
          interfaceName="org.abchip.mimo.database.query.QueryParserRegistry"/>
      <services
          xsi:type="mimo-app:ServiceRegistry"
          className="org.abchip.mimo.database.base.BaseQueryWriterRegistryImpl"
          interfaceName="org.abchip.mimo.database.query.QueryWriterRegistry"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.database.base.BaseDatabaseManagerImpl"
          interfaceName="org.abchip.mimo.database.DatabaseManager"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.database.base.BaseConnectionManagerImpl"
          interfaceName="org.abchip.mimo.database.connection.ConnectionManager"/>
    </modules>
    <modules
        name="Test">
      <services
          xsi:type="mimo-app:ServiceTester"
          className="org.abchip.mimo.database.test.TestDatabaseSuiteLauncherImpl"/>
    </modules>
  </components>
  <components
      name="Language">
    <modules
        name="Base">
      <services
          xsi:type="mimo-app:ServiceRegistry"
          className="org.abchip.mimo.language.base.BaseLanguageParserRegistryImpl"
          interfaceName="org.abchip.mimo.language.LanguageParserRegistry"/>
      <services
          xsi:type="mimo-app:ServiceRegistry"
          className="org.abchip.mimo.language.base.BaseLanguageLinearizerRegistryImpl"
          interfaceName="org.abchip.mimo.language.LanguageLinearizerRegistry"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.language.base.BaseLanguageManagerImpl"
          interfaceName="org.abchip.mimo.language.LanguageManager"/>
    </modules>
    <modules
        name="GF">
      <services
          xsi:type="mimo-app:ServiceRegistryEntry"
          className="org.abchip.mimo.language.gf.GFLanguageParserImpl"
          interfaceName="org.abchip.mimo.language.LanguageParser"
          name="GF"
          vendor="GrammaticalFrameWork"
          version="3.9"/>
      <services
          xsi:type="mimo-app:ServiceRegistryEntry"
          className="org.abchip.mimo.language.gf.GFLanguageLinearizerImpl"
          interfaceName="org.abchip.mimo.language.LanguageLinearizer"
          name="GF"
          vendor="GrammaticalFrameWork"
          version="3.9"/>
    </modules>
    <modules
        name="Test">
      <services
          xsi:type="mimo-app:ServiceTester"
          className="org.abchip.mimo.language.test.TestLanguageSuiteLauncherImpl"/>
    </modules>
    <modules
        name="LP">
      <services
          xsi:type="mimo-app:ServiceRegistryEntry"
          className="org.abchip.mimo.language.lp.LPClassifierImpl"
          interfaceName="org.abchip.mimo.mining.classification.Classifier"
          name="GF"
          vendor="LingPipe"
          version="3.9"/>
    </modules>
  </components>
  <components
      name="Server">
    <config>
      <entities
          xsi:type="mimo-net:SocketConfig"
          address="127.0.0.1"
          port="8080"/>
    </config>
    <hooks className="org.abchip.mimo.core.http.HttpActivatorHook"/>
    <modules
        name="Service">
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.session.GitHubRedirectServlet"
          alias="/biz/service/gitHubRedirect"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.session.GitHubResponseServlet"
          alias="/biz/service/gitHubResponse"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.session.GoogleRedirectServlet"
          alias="/biz/service/googleRedirect"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.session.GoogleResponseServlet"
          alias="/biz/service/googleResponse"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.session.LinkedInRedirectServlet"
          alias="/biz/service/linkedInRedirect"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.session.LinkedInResponseServlet"
          alias="/biz/service/linkedInResponse"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.session.LoginServlet"
          alias="/biz/service/login"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.session.LogoutServlet"
          alias="/biz/service/logout"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.session.StatusServlet"
          alias="/biz/service/status"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.DeleteServlet"
          alias="/biz/service/delete"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.FindServlet"
          alias="/biz/service/find"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.FindNamesServlet"
          alias="/biz/service/findNames"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.LookupServlet"
          alias="/biz/service/lookup"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.SaveServlet"
          alias="/biz/service/save"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.ui.http.LookupMenuServlet"
          alias="/biz/service/lookupMenu"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.ui.http.LookupSchemaServlet"
          alias="/biz/service/lookupSchema"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.ui.http.LookupToolbarServlet"
          alias="/biz/service/lookupToolbar"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.ui.http.LookupFormServlet"
          alias="/biz/service/lookupForm"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.ui.http.LookupQueryServlet"
          alias="/biz/service/lookupQuery"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.ui.http.LookupContextMenuServlet"
          alias="/biz/service/lookupContextMenu"/>
    </modules>
  </components>
  <components
      name="Biz">
    <config>
      <entities
          xsi:type="biz:BizProviderConfig"
          url="https://localhost:8443/service">
        <publicUser
            user="guest"
            password="ofbiz"/>
      </entities>
    </config>
    <commands
        className="org.abchip.biz.core.ofbiz.command.BizCommandProviderImpl"/>
    <modules
        name="Base">
      <services
          xsi:type="mimo-app:ServiceRegistryEntry"
          className="org.abchip.biz.core.ofbiz.http.OFBizHttpEntityProviderImpl"
          interfaceName="org.abchip.mimo.entity.EntityProvider"
          name="*OFBIZ"
          vendor="ABChip"
          version="0.1.0"/>
    </modules>
    <modules
        name="Party">
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.biz.party.ofbiz.ImportPeopleServlet"
          alias="/biz/service/importPeople"/>
    </modules>
    <modules
        name="Product">
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.biz.product.ofbiz.ImportProductCategoriesServlet"
          alias="/biz/service/importProductCategories"/>
    </modules>
    <modules
        name="Test">
      <services
          xsi:type="mimo-app:ServiceTester"
          className="org.abchip.biz.party.test.TestBizPartySuiteLauncher"/>
    </modules>
  </components>
  <contextDescription
      id="abchip-net"
      dataPath="/mimo/abchip-net"
      user="mimo">
  </contextDescription>
</mimo-app:Application>
